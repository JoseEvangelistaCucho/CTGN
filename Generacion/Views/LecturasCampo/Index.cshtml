    function ObtenerDetalleFormato() {
        var idReporte = datosOperario.IdSitio + "-ELD.CTL-OM-001-" + obtenerFecha("fecha");
        var filasIds = @Html.Raw(JsonConvert.SerializeObject(idReferencias));

        const datos = [];

        filasIds.forEach(filaId => {
            const filaPrincipal = document.querySelector(`[data-id^="${filaId}-G${1}"]`);

            const cantidadElementos = (filaPrincipal.children.length - 3) * 2;
            console.log("cantidadElementos : ", cantidadElementos);

            for (let i = 2; i <= cantidadElementos; i = i + 2) {
                var idDetalle = datosOperario.IdSitio + "-Det-" + filaId + "-1-" + i.toString() + "-" + obtenerFecha("fecha");

                const celda = filaPrincipal.querySelector(`td[id="${filaId}_${i}"]`);
                console.log("celda : ", celda);

                const dato = {
                    idDetalleCampo: idDetalle,
                    detalle: parseFloat(celda.querySelector('input').value || 0),
                    fila: i,
                    numeroGenerador: 1,
                    hora: i + ':00',
                    sitio: datosOperario.IdSitio,
                    idSubtituloCampo: filaId,
                    fecha: obtenerFecha('fechaVista'),
                    idReporteCampo: idReporte
                };

                datos.push(Object.assign({}, dato));
            }
        });


        loader.style.display = 'block';

        return fetch('@Url.Action("GuardarDatosConsola", "LecturasCampo")', {
            method: "POST",
            headers: {
                "Content-Type": "application/json"
            },
            body: JSON.stringify(datos)
        })
            .then(function (response) {
                console.log('ejecutado correctamente: ', response);
                return response.json();
            })
            .then(function (data) {
                loader.style.display = 'none';

                if (data.respuesta.idRespuesta === 0 || data.respuesta.idRespuesta === 1) {
                    Swal.fire({
                        title: "Success!!",
                        text: "Se guardaron los datos.",
                        icon: "success",
                        button: "Aww yiss!",
                        willClose: () => {
                            location.reload();
                        }
                    });

                } else {
                    Swal.fire({
                        title: "Error",
                        text: data.respuesta.mensaje,
                        icon: "error",
                        button: "OK",
                    });
                }
            })
            .catch(function (error) {
                loader.style.display = 'none';
                console.error("Error:", error);
            });
    }



